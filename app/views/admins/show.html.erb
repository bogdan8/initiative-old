<div class="container">
  <div class="row">
    <div class="col-lg-11 col-xs-12">
      <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
          <p class="user-panel-show-title">
            Інформація про адміна
          </p>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-4 col-md-4 col-xs-12">
          <p class="user-panel-show-names-user-information">Фото: </p>
        </div>
        <div class="col-lg-8 col-md-8 col-xs-12">
          <div class="user-panel-show-new-file-block">
            <% unless @admin.avatar.url == '/images/original/missing.png' %>
                <%= image_tag @admin.avatar.url, class: 'user-panel-show-block-show-images' %>
            <% end %>
          </div>
        </div>
      </div>

      <div class="row">
        <div class="col-lg-4 col-md-4 col-xs-12">
          <p class="user-panel-show-names-user-information">
            <%= t('views.user_panel.show.name') %>:
          </p>
        </div>
        <div class="col-lg-8 col-md-8 col-xs-12">
          <p class="user-panel-show-names-user-information">
            <%= @admin.name %>
          </p>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-4 col-md-4 col-xs-12">
          <p class="user-panel-show-names-user-information">
            <%= t('views.user_panel.show.post') %>:
          </p>
        </div>
        <div class="col-lg-8 col-md-8 col-xs-12">
          <p class="user-panel-show-names-user-information">
            <%= @admin.post %>
          </p>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-4 col-md-4 col-xs-12">
          <p class="user-panel-show-names-user-information">
            <%= t('views.user_panel.show.phone') %>:
          </p>
        </div>
        <div class="col-lg-8 col-md-8 col-xs-12">
          <p class="user-panel-show-names-user-information">
            <%= @admin.phone %>
          </p>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-4 col-md-4 col-xs-12">
          <p class="user-panel-show-names-user-information">
            <%= t('views.user_panel.show.email') %>:
          </p>
        </div>
        <div class="col-lg-8 col-md-8 col-xs-12">
          <p class="user-panel-show-names-user-information">
            <%= @admin.email %>
          </p>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-4 col-md-4 col-xs-12">
          <p class="user-panel-show-names-user-information">
            Скільки разів заходив:
          </p>
        </div>
        <div class="col-lg-8 col-md-8 col-xs-12">
          <p class="user-panel-show-names-user-information">
            <%= @admin.sign_in_count %>
          </p>
        </div>
      </div>
    </div>
  </div>
</div>