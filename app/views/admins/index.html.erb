<h2>Список адміністраторів:</h2> <br/>
<% @admins.each do |items| %>
    <h4>Емайл: <%= items.email %></h4>
    <p>
      <%= link_to 'Показати', admin_path(items), class: 'btn btn-default' %> |

      <%= link_to 'Редагувати', edit_admin_path(items), class: 'btn btn-warning' %> |

      <%= link_to 'Видалити', admin_path(items), method: :delete, class: 'btn btn-danger', data: {confirm: 'Ви впевнені?'} %>
    </p>

<% end %>
