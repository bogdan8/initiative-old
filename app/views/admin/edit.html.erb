<h2>Редагувати</h2>
<%= form_for :user, url: admin_path(@admin), method: :put do |f| %>
    <p>
      <%= f.label 'Email' %>
      <%= f.text_field :email %>
    </p>
    <p>
      <%= f.label 'Пароль' %>
      <%= f.text_field :password %>
    </p>
    <p>
      <%= f.submit 'редагувати' %>
    </p>
<% end %>
